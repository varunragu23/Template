{
  "code-runner.executorMap": {
    "javascript": "node",
    "python": "python",
    "perl": "perl",
    "c": "cd $dir && g++-9 -g -DDEBUG -Wall -Wextra -Wpedantic -Wconversion -std=c++11 $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "cpp": "cd $dir && g++-9 -g -DDEBUG -Wall -Wextra -Wpedantic -Wconversion -std=c++11 -Wshadow -Wformat=2 -Wfloat-equal -Wlogical-op -Wshift-overflow=2 -Wduplicated-cond -Wcast-qual -Wcast-align -D_FORTIFY_SOURCE=2 -fsanitize=address -fsanitize=undefined -fno-sanitize-recover -fstack-protector $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "java": "cd $dir && javac $fileName && java $fileNameWithoutExt"
  },
  "io-run.executorMap.darwin": {
    ".cpp": {
      "compileCmd": "c++-9 -g -DDEBUG -DONLINE_JUDGE -Wall -Wextra -Wpedantic -Wconversion -std=c++11 -Wfatal-errors ${codeFile} -o ${codeFileNoExt}",
      "runCmd": "${dirCodeFileNoExt} <${inputFile} >${outputFile}",
      "cleanupCmd-delete": "rm ${codeFileNoExt}",
      "errorTracer": "lldb"
    },
    ".java": {
      "compileCmd": "javac -encoding UTF-8 ${codeFile}",
      "runCmd": "java ${codeFileNoExt} <${inputFile} >${outputFile}",
      "cleanupCmd": "rm ${codeFileNoExt.class}"
    },
    ".py": {
      "runCmd": "python ${codeFile} <${inputFile} >${outputFile}"
    }
  },
  "files.associations": {
    "iostream": "cpp"
  },
  "C_Cpp.clang_format_style": "Google",
  "files.autoSave": "afterDelay",
  "code-runner.clearPreviousOutput": true,
  "workbench.iconTheme": "vscode-icons",
  "C_Cpp.updateChannel": "Insiders",
  "explorer.confirmDelete": false,
  "workbench.startupEditor": "newUntitledFile",
  "vsicons.dontShowNewVersionMessage": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "io-run.continueOnFails": true,
  "io-run.diffIgnoreSpaces": true,
  "io-run.enableAnalytic": false,
  "io-run.showErrorOutputOnFails": true,
  "io-run.showDiffInOutputPanel": true,
  "io-run.cleanupAfterRun": false,
  "io-run.timeLimit": 4,
  "diffEditor.renderSideBySide": false,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": true
  },
  "git.enableSmartCommit": true,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.fontFamily": "Source Code Pro for Powerline",
}